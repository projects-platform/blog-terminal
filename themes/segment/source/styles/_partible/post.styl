#post
  margin 0 auto
  width 70%

  // background-color #F8F8F8
  div
    padding 30px 0
    margin 0 auto

  .main
    width 700px

  h1.title
    font-size 30px
    color #333
    line-height 1.3
    position relative
    font-weight bold

  .stuff
    margin 20px 0
    color #666
    font-size 13px

    span
      display inline-block
      margin-right 20px

  img
    display block
    margin 0 auto
    width 100%
    cursor pointer
